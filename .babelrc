{
    "presets": [
        "@babel/preset-env",
        ["@babel/preset-react", {"runtime": "automatic"}]
    ],
    "plugins": [
        [
            "module-resolver",
            {
                "root": [
                    "./"
                ],
                "alias": {
                    "@": "./src",
                    "@hooks": "./src/hooks",
                    "@style": "./src/style",
                    "@pose": "./src/pose",
                    "@static": "./static",
                    "@util": "./src/util"
                }
            }
        ]
    ]
}